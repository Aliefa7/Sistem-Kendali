>> % --- NOMOR 1 ---
>> A = [ 0 1; -2 -3 ];
>> B = [ 1 0; 3 -2 ];
>> C = [ 1 -1; -2 2 ];
>> A + B
ans =

     1     1
     1    -5

>> A - B
ans =

    -1     1
    -5    -1

>> A'
ans =

     0    -2
     1    -3

>> inv(A)
ans =

   -1.5000   -0.5000
    1.0000         0

>> diag(A),diag(B)
ans =

     0
    -3

ans =

     1
    -2

>> det(A),det(B)
ans =

     2

ans =

    -2

>> det(A*B)
ans =

   -4.0000

>> eig(A)
ans =

    -1
    -2

